{
	"image": "mcr.microsoft.com/devcontainers/universal:2",
	"hostRequirements": {
	  "cpus": 2
	},
	"waitFor": "onCreateCommand",
	"updateContentCommand": "npm install",
	"postCreateCommand": "",
	"postAttachCommand": {
	  "server": "npm start"
	},
	"customizations": {
	  "codespaces": {
		"openFiles": [
		  "src/Components/App.tsx"
		]
	  }
	},
	"portsAttributes": {
	  "9000": {
		"label": "Application"
	  }
	},
	"forwardPorts": [9000]
  }
  